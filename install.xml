<!--
Installation model
    1. by path & name
    path - absolute path to the file
    name - file name
    2. type
    * systemd-tmpfiles  - install $(project.name) to /usr/lib/systemd/tmpfiles.d/
     -->

<install>
    <item type = "systemd-tmpfiles" />
    <item path = "/etc/fty/fty-security-wallet" name = "../configuration.json" />
    <item path = "/usr/share/bios/setup/" name = "../src/30-secw-update-database-right.sh" />
</install>